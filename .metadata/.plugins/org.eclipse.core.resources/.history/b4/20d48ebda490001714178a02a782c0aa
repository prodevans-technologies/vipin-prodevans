package com.prodevans.condition

object PatternMatching 
{
  
  def main(args : Array[String])
  {
    
    
    def matchTestInt(x:Int): String = x match
    {
      case 1=> "one"
      case 2=> "two"
      case _=> "many"
    }
    
    def matchTestString(x:Int): String = x match
    {
      case 1=> "one"
      case 2=> "two"
      case _=> "many"
    }
    
  }
  
}